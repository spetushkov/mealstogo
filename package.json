{
  "name": "mealstogo",
  "version": "0.0.1",
  "private": true,
  "description": "MealsToGo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:spetushkov/mealstogo.git"
  },
  "author": "VisionWorks",
  "homepage": "/",
  "bugs": {
    "url": "https://github.com/spetushkov/mealstogo/issues"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf coverage && rimraf ios/Pods && rimraf android/.gradle && rimraf node_modules",
    "commit": "git add . && git commit -m 'feat: :rocket:'",
    "compile": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "lint:license": "license-checker --production --json --out licenses.json",
    "lint:node_modules": "npm i --package-lock-only && ./nm-lint.sh",
    "lint:staged": "lint-staged",
    "open:android": "open -a /Applications/Android\\ Studio.app ./android",
    "open:debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "open:ios": "open ios/$npm_package_name.xcworkspace",
    "postinstall": "husky install",
    "run:android": "react-native run-android",
    "run:android:device:install": "adb reverse tcp:8081 tcp:8081",
    "run:android:device:list": "adb devices",
    "run:ios": "react-native run-ios",
    "run:ios:install": "npx pod-install ios",
    "start": "react-native start",
    "test": "yarn test:unit",
    "test:all": "yarn test:unit && yarn test:integration",
    "test:coverage": "jest --config jest.config.js --group=unit --coverage",
    "test:dev": "jest --config jest.config.js --group=unit --watch",
    "test:integration": "jest --config jest.config.integration.js --group=integration",
    "test:staged": "jest --config jest.config.integration.js --onlyChanged",
    "test:unit": "jest --config jest.config.js --group=unit"
  },
  "keywords": [
    "react native"
  ],
  "dependencies": {
    "@visionworksco/react-middleware": "^1.7.0",
    "axios": "^0.25.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "envalid": "^7.2.2",
    "i18next": "^21.6.11",
    "react": "17.0.2",
    "react-i18next": "^11.15.3",
    "react-native": "0.67.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt-cli": "^16.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "27.4.0",
    "@types/react-native": "^0.66.15",
    "@types/react-redux": "^7.1.22",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "27.5.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "27.5.0",
    "jest-extended": "^2.0.0",
    "jest-mock-extended": "^2.0.4",
    "jest-runner-groups": "^2.1.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.3.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-package": "^1.3.0",
    "react-test-renderer": "17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "27.1.3",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
